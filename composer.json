{
  "name": "bjoern3003/github-api-bundle",
  "description": "A bridge for using Maxikg/github-api in Symfony 4 / Kimai.",
  "minimum-stability": "stable",
  "keywords": ["github", "api client", "symfony 4", "kimai", "kimai-plugin"],
  "license": "MIT",
  "authors": [
    {
      "name": "Max Walsch",
      "email": "max.walsch@gmail.com"
    },
    {
      "name": "Bjoern Fischer",
      "email": "info@x-volution.de"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/http-kernel": ">4.0",
    "symfony/dependency-injection": ">4.0",
    "symfony/config": ">4.1",
    "php-http/guzzle6-adapter": "~2.0"
    "knplabs/github-api": "~2.12.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "extra": {
    "kimai": {
      "require": "1.5",
      "version": "1.0",
      "name": "Github API Bundle"
    }
  }
}
